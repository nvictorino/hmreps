header.site-header
  div.navbar
    div.container.d-flex.justify-content-between
      a(href='./').navbar-brand.d-flex.align-items-left
        img(src="/images/ochalogo_0.png" alt="Image Title")
      
      if menuContent != null
        nav
          ul
            each menuLink in menuContent.data.menu_links
              li
                a(href=PrismicDOM.Link.url(menuLink.link, ctx.linkResolver))
                  != PrismicDOM.RichText.asText(menuLink.label)